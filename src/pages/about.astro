---
import BaseLayout from "../layouts/BaseLayout.astro";
import "../styles/about.css";
import "../styles/globals.css";

import { Image } from "astro:assets";
import giphy from "../assets/200w.webp";

const pageTitle: string = "2D World";
---

<!-- 2D World -->
<BaseLayout title={pageTitle}>
    <p style="font-weight: 600;">2D World Comming Soon 🧑‍🌾</p>
    <Image
        src={giphy}
        alt="The capital letters E, in navy blue, and the letter N, in purple, on a yellow background."
        loading={"lazy"}
        quality={100}
        class={"comming-soon-giph"}
    />
</BaseLayout>
